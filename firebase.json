{
  "apphosting": {
    "backendId": "catering",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "firestore": {
    "rules": "firestore.rules",
    "databases": {
      "catering": {
        "rules": "firestore.rules"
      }
    }
  },
  "database": {
    "rules": "database.rules.json"
  }
}
